{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "rules": {
        "react/prop-types": 0,
        "react/react-in-js-scope": 0,
        "react/react-in-jsx-scope": 0
    },
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}